    "version": "0.2.0",
    "configurations": [
        {
            "name": "Start App",
            "cwd": "${workspaceFolder}/app",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "Start Web server",
            "cwd": "${workspaceFolder}/server",
            "command": "pnpm run dev",
            "request": "launch",
            "type": "node-terminal"
        },
        {
            "name": "Start App (profile mode)",
            "cwd": "${workspaceFolder}/app",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "Start App (release mode)",
            "cwd": "${workspaceFolder}/app",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
}